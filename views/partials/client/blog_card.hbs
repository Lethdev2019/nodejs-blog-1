<div class="col-sm-6 col-lg-3 col-md-4 {{#each blog.tags}}{{this}}{{/each}}">
    <div class="gal-detail thumb">
        <a href="/blog/{{blog.url}}" class="image-popup" title="">
            <img src="{{blog.cover_img}}" class="thumb-img" alt="">
        </a>
        <h4>{{blog.title}}</h4>
        <p class="text-muted">
            {{blog.small_desc}}
        </p>
    </div>
</div>